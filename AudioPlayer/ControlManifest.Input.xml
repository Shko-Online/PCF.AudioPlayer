<manifest>
  <control 
    namespace="ShkoOnline" 
    constructor="AudioPlayer" 
    version="1.0.1" 
    display-name-key="AudioPlayer_Display_Key" 
    description-key="AudioPlayer_Desc_Key" 
    control-type="standard" 
  >
    <external-service-usage 
      enabled="false" 
    />
    <property 
      name="src" 
      display-name-key="Src_Display_Key" 
      description-key="Src_Desc_Key" 
      of-type="SingleLine.Text" 
      usage="bound" 
      required="true" 
    />
    <resources>
      <code 
        path="index.ts" 
        order="1"
      />
      <resx 
        path="strings/AudioPlayer.1033.resx" 
        version="1.0.0" 
      />
      <css 
        path="css/AudioPlayer.css" 
        order="1" 
      />
    </resources>
  </control>
</manifest>
